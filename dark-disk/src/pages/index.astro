---
import ThemeController from "../components/ThemeController.astro";
import Layout from "../layouts/Layout.astro";
import { ArrowDownCircleIcon, ArrowUpCircleIcon, LinkedinIcon, GithubIcon } from "astro-feather";
---

<Layout title="Gabriel Audette | Portfolio">
    <main class="h-screen">
        <section id="hero" class="hero min-h-screen relative">
            <div class="hero-content">
                <div class="flex flex-col gap-4 items-center">
                    <h1 class="text-5xl font-bold">Hello, I'm Gabriel Audette</h1>
                    <p class="text-2xl">I'm a software developer based in Montreal, Canada.</p>
                    <div class="flex items-center gap-4">
                        <a class="text-secondary" href="https://www.linkedin.com/in/gabriel-audette/" target="_blank">
                            <LinkedinIcon />
                        </a>
                        <a class="text-secondary" href="https://github.com/gabaudette" target="_blank">
                            <GithubIcon />
                        </a>
                    </div>
                </div>
            </div>
            <a href="#projects" class="absolute bottom-10 mb-4 animate-bounce text-secondary">
                <ArrowDownCircleIcon size={50} />
            </a>
        </section>
        <section id="projects" class="projects min-h-screen relative">
            <div class="flex items-center justify-center w-full">
                <a href="#hero" class="absolute top-10 mt-4 animate-bounce text-secondary">
                    <ArrowUpCircleIcon size={50} />
                </a>
            </div>
            <div class="absolute top-52 mx-8">
                <h2 class="text-4xl font-bold mb-8">Projects</h2>
                <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-12">
                    <div class="card card-compact bg-base-100 max-w-96 shadow-lg">
                        <div class="card-body">
                            <card-title class="text-2xl font-semibold">Project (unannounced)</card-title>
                            <div class="card-actions">
                                <div class="flex flex-col">
                                    <p class="text-lg leading-normal mb-2">Description</p>
                                    <p class="text-base h-24">
                                        A project that I'm currently working on in my spare time. More details will be available soon.
                                    </p>
                                    <div class="divider"></div>
                                    <h3 class="text-lg leading-normal my-2">Technologies used</h3>
